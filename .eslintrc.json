{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "@typescript-eslint/no-unused-vars": "error",
        "prettier/prettier": ["error", { "singleQuote": true }],
        "@typescript-eslint/consistent-type-definitions": ["error"]
    }
}
